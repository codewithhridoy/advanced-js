{
  "name": "@codewithhridoy/tailwind-browser",
  "version": "1.0.1",
  "sideEffects": false,
  "description": "Tailwind browser helps you to develop efficiently for cross-browser platform",
  "author": "Hridoy Ahmed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codewithhridoy/advanced-js.git"
  },
  "bugs": "https://github.com/codewithhridoy/advanced-js/issues",
  "homepage": "https://github.com/codewithhridoy/advanced-js#readme",
  "type": "module",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "release": "pnpm run build && changeset publish",
    "build": "rollup -c",
    "publish-packages": "pnpm publish --access public --no-git-checks",
    "test:publish-packages": "pnpm publish --access public --no-git-checks --provenance --dry-run",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown"
  },
  "devDependencies": {
    "rollup": "3.28.0",
    "tailwindcss": "3.3.3"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "keywords": [
    "tailwindcss",
    "cross-browser",
    "tailwind-browser",
    "tailwind-cross-browser",
    "next",
    "react",
    "firefox",
    "chrome",
    "safari"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.6.12"
}
